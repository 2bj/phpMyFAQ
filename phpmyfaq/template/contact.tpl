<h2>{msgContact}</h2>
	<p>{msgContactOwnText}</p>
	<p><strong>{msgContactEMail}</strong></p>
	<form action="{writeSendAdress}" method="post" style="display: inline">
    
	<div class="row"><span class="label">{msgNewContentName}</span>
    <input class="inputfield" type="text" name="name" size="40" /></div>
	
	<div class="row"><span class="label">{msgNewContentMail}</span>
    <input class="inputfield" type="text" name="email" size="40" /></div>
	
	<div class="row"><span class="label">{msgMessage}</span>
    <textarea class="inputarea" cols="38" rows="5" name="question"></textarea></div>
	
	<div class="row"><span class="label">&nbsp;</span>
    <input class="submit" type="submit" name="submit" value="{msgS2FButton}" /></div>
	
	</form>
    
